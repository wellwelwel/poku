{
  "all": true,
  "include": ["src/**"],
  "exclude": [
    "src/@types",
    "src/bin",
    "src/configs",
    "src/polyfills",
    "src/index.ts",
    "benchmark",
    "ci",
    "fixtures",
    "lib",
    "test",
    "tools",
    "website"
  ],
  "extension": [".ts"],
  "reporter": ["text", "lcov", "cobertura"],
  "clean": true,
  "branches": 85,
  "statements": 90,
  "lines": 90,
  "functions": 95,
  "checkCoverage": true,
  "import": ["tsx"]
}
