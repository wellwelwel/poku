{
  "clean": true,
  "all": true,
  "include": ["src/**"],
  "exclude": [
    "src/@types",
    "src/bin",
    "src/configs",
    "src/polyfills",
    "src/modules/index.ts",
    "src/modules/helpers/exit.ts",
    "src/modules/helpers/get-pids.ts",
    "src/modules/helpers/list-files-sync.ts",
    "src/modules/helpers/kill.ts",
    "src/modules/helpers/log.ts",
    "src/services/pid.ts",
    "src/parsers/get-runner.ts",
    "src/parsers/get-runtime.ts"
  ],
  "reporter": ["text", "lcov", "cobertura"],
  "branches": 95,
  "statements": 95,
  "lines": 95,
  "functions": 95,
  "exclude-after-remap": true,
  "checkCoverage": true,
  "import": ["tsx"],
  "extension": [".ts"]
}
